{
  "version": 2,
  "builds": [
    {
      "src": "mvp-breast-usg/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "cd mvp-breast-usg/frontend && npm install",
        "buildCommand": "cd mvp-breast-usg/frontend && npm run build",
        "outputDirectory": "mvp-breast-usg/frontend/build"
      }
    },
    {
      "src": "mvp-breast-usg/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/mvp-breast-usg/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/mvp-breast-usg/frontend/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/mvp-breast-usg/frontend/build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/mvp-breast-usg/frontend/build/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/mvp-breast-usg/frontend/build/index.html"
    }
  ]
}